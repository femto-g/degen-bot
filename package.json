{
  "name": "degen-bot",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "tsc-watch -p tsconfig.json --onSuccess \"cross-env NODE_ENV=development node ./dist/index.js\" --noClear",
    "deploy": "node ./dist/deploy.js",
    "build": "tsc -p tsconfig.json",
    "docker:dev:up": "docker compose up --watch",
    "docker:dev:down": "docker compose down",
    "docker:rebuild": "docker compose build --no-cache",
    "docker:dev:up:clean": "npm run docker:rebuild && npm run docker:dev:up",
    "migrate:dev": "dotenv -e .env.migrate -- npx prisma migrate dev",
    "docker:run:db": "docker compose run --service-ports db",
    "docker:stop:db": "docker compose stop db",
    "docker:run:db:detached": "docker compose run -d --service-ports db"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/chart.js": "^2.9.41",
    "@types/luxon": "^3.4.2",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "tsc-watch": "^6.2.1",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@prisma/client": "^6.2.1",
    "bottleneck": "^2.19.5",
    "chart.js": "^4.4.7",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^8.0.0",
    "http-status-codes": "^2.3.0",
    "luxon": "^3.5.0",
    "node-cron": "^3.0.3",
    "prisma": "^6.2.1",
    "quickchart-js": "^3.1.3",
    "redis": "^4.7.0"
  }
}
